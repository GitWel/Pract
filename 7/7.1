#include "pch.h"
#include <iostream>
#include <string>
using namespace std;

class Animal {
protected:
	string species;
	int age;
	string name;
};

class Cat:public Animal {
public:
	string voice = "meow";
	Cat(string a, int b, string c) {
		a=species;
		b = age;
		c = name;
	}

};

class Dog :public Animal {
public:
	string voice = "bark";
	Dog(string a, int b, string c) {
		a = species;
		b = age;
		c = name;
	}

};

class Parrot :public Animal {
public:
	string voice = "Hello";
	Parrot(string a, int b, string c) {
		a = species;
		b = age;
		c = name;
	}

};
